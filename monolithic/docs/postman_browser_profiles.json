		{
			"name": "Browser Profiles",
			"item": [
				{
					"name": "List All Profiles",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/v1/profiles?limit=50&offset=0",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "profiles"],
							"query": [
								{
									"key": "limit",
									"value": "50",
									"description": "Number of profiles to return"
								},
								{
									"key": "offset",
									"value": "0",
									"description": "Pagination offset"
								},
								{
									"key": "status",
									"value": "active",
									"disabled": true,
									"description": "Filter by status: active, inactive, archived"
								},
								{
									"key": "folder",
									"value": "Production",
									"disabled": true,
									"description": "Filter by folder name"
								}
							]
						}
					},
					"response": []
				},
